{
    "name": "MediaMTX Add-on",
    "version": "1.9.3",
    "slug": "mediamtx",
    "description": "Create RTSP server to broadcast streams, thanks to @aler9 - https://github.com/aler9/rtsp-simple-server",
    "arch": ["armv7", "aarch64", "amd64", "armhf"],
    "startup": "application",
    "boot": "auto",
    "ports": {
        "8559/tcp": 8554,
        "1935/tcp": 1935,
        "9997/tcp": 9997
    },
    "map": ["addon_config:rw"],
    "homeassistant_api": true,
    "options": {},
    "schema": {},
    "image": "bluenviron/mediamtx",
    "ingress": false,
    "panel_icon": "mdi:cctv",
    "panel_title": "RTSP Server",
    "panel_admin": true,
    "gpio": false,
    "udev": false,
    "stage": "stable",
    "healthcheck": "http://localhost:9997/",
    "apparmor": true,
    "timeout": 10,
    "init": true,
    "advanced": false,
    "auto_update": true
}
